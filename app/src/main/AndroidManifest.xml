<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mtripode.pettest1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PetTest1"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.customerDoctorsMenu.CustomerDoctorsActivity"></activity>
        <activity android:name=".ui.home.doctor.HomeDoctorActivity" />
        <activity android:name=".ui.addupdatepet.AddUpdatePetActivity" />
        <activity android:name=".ui.petmenu.PetMenuActivity" />
        <activity
            android:name=".ui.home.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.PetTest1.NoActionBar" />
        <activity
            android:name=".ui.register.RegisterActivity"
            android:label="@string/title_activity_register"
            android:theme="@style/Theme.PetTest1.NoActionBar" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>